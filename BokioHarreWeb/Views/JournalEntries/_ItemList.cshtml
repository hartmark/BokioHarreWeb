@model IEnumerable<BokioHarreWeb.DataContract.Item>

<div class="mt-4">
    @if (Model.Any())
    {
        foreach (var item in Model)
        {
            @await Html.PartialAsync("_Item", item)
        }
    }
    else
    {
        <div class="alert alert-warning">No items found.</div>
    }
</div>